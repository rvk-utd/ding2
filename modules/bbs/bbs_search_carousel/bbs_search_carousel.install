<?php

/**
 * @file
 * Install, update and uninstall functions for the ting_search_carousel module.
 */

/**
 * Implements hook_install().
 */
function bbs_search_carousel_install() {
  @drupal_mkdir('public://bbs_search_carousel');
}

/**
 * Implements hook_uninstall().
 */
function bbs_search_carousel_uninstall() {
  @drupal_rmdir('public://bbs_search_carousel');
}
